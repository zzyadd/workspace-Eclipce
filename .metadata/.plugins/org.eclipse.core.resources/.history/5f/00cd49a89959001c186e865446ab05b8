	 public static void fact ( int y) {
		 int z =1 ;
		 for (int i =y ; i>=1; i--) {
			 z *=i ;
		 }
		 System.out.println(z);
	 }
	public static void pprint(int x , String y ) {
	 for ( int i =1 ; i<=x ; i++ ) {
		 System.out.print(y);
	 }
	 System.out.println();
	}	
	
	
	
