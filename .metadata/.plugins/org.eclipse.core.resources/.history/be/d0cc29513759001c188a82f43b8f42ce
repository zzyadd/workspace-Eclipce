package assignment1programming1;
import java.util.*;
import java.util.Scanner;
public class Quiz1 {

	public static void main(String[] args) {
 //EX:2
		 Scanner s = new Scanner (System.in);
		
		//EX:3
		int no ;
		System.out.println("Enter a number :");
		 no = s.nextInt();
		do {
		if (no == 0) {
			break ;
		} 
		if (no >2 && no <20) {
			System.out.println("the number is in range ");
		} else {
			System.out.println("the number out of range ");
		}System.out.println("Enter a number :");
		 no = s.nextInt();}
		while(no!=0);
		


	}

}
