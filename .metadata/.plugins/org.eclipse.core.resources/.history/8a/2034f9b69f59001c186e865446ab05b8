package assignment1programming1;
import java.util.*;

public class sssal {
	public static void first() {
	System.out.println("  ____ ");
	System.out.println(" /    \\");
	System.out.println("/      \\");
	System.out.println("\\      /");
	System.out.println(" \\____/ ");}
	public static void second () {
		System.out.println();
		System.out.println("\\       /");
		System.out.println(" \\_____/");
		System.out.println("+-------+");
	}
	public static void therd () {
		System.out.println("  ____ ");
		System.out.println(" /    \\");
		System.out.println("/      \\");
		System.out.println("| stop |");
		System.out.println("\\      / ");
		System.out.println(" \\____/");
	}
	public static void fourth() {
		System.out.println("  ____ ");
		System.out.println(" /    \\");
		System.out.println("/      \\");
		System.out.println("+-------+");
	}
	public static void printx(int x ) {
		for (int i =0 ; i<=x ; i++ ) {
			System.out.println(x);
		}
	}
	 public static void fact ( int y) {
		 int z =1 ;
		 for (int i =y ; i>=1; i--) {
			 z *=i ;
		 }
		 System.out.println(z);
	 }
	public static void pprint(int x , String y ) {
	 for ( int i =1 ; i<=x ; i++ ) {
		 System.out.print(y);
	 }
	 System.out.println();
	}	
	public static void my (int x , int z , int y) {
		System.out.println(z+ "=3   and 2-1 = "+ (y - x));
	}
	public static void ste(int x ) {
		for (int i =1 ; i<=x ; i++ ) {
			System.out.print("["+i+"]");}
	}
			public static void parametrt(int x) {
				for (int i =2 ; i<x ; i++) {
					if (x %i ==0) {
						System.out.println("number isnot parametrt ");
						break;
						}
						
							System.out.println("num is parametrt ");
					}
				}
			public static void pow (int x , int y) {
				int z =x ;
				for(int i=y ; i>1 ; i--) {
					z*=x;
				}
				System.out.println(z);
			}
					
	public static void main(String[] args) {
		 Scanner sscc = new Scanner (System.in);

		 pow(5,2);
	}
	
}
